generator client {
  provider = "prisma-client-js"
}

datasource neondb {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model playing_with_neon {
  id    Int    @id @default(autoincrement())
  name  String
  value Float? @neondb.Real
}

model test_kill_me {
  id    Int    @id @default(autoincrement())
  name  String
  value Float? @neondb.Real
}
